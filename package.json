{
  "name": "js-test-gen-atom",
  "main": "./lib/js-test-gen",
  "version": "0.1.0",
  "description": "Js-test-gen-atom generates test templates from your default/named exports.",
  "keywords": [
    "unit-test",
    "mocha",
    "jest",
    "js-test-gen",
    "atom-plugin",
    "javascript"
  ],
  "activationCommands": {
    "atom-workspace": [
      "js-test-gen: generate-test-template",
      "js-test-gen: generate-test-from-selection"
    ]
  },
  "repository": "https://github.com/js-test-gen/js-test-gen-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "js-test-gen": "^1.0.0",
    "prettier": "^1.10.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "mock-fs": "^4.4.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1"
  }
}
