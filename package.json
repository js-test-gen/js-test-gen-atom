{
  "name": "js-test-gen-atom",
  "main": "./lib/js-test-gen",
  "version": "0.0.0",
  "description": "An atom plugin using js-test-gen to create test boilerplate",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "js-test-gen:generateTestTemplate",
      "js-test-gen:generateTest"
    ]
  },
  "repository": "https://github.com/atom/js-test-gen",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["eslint", "prettier-eslint --write", "git add"]
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "prettier": "^1.10.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.17.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1"
  }
}
